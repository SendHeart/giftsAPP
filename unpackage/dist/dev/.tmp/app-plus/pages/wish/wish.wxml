<view><uni-nav-bar vue-id="1" fixed="{{true}}" color="#fff" background-color="#1d1d1d" bind:__l="__l"></uni-nav-bar><view class="userinfo_my" style="{{('background-image: url('+wish_banner+');background-size:cover;')}}" hidden="{{!shareflag}}"><text class="userinfo-nickname" style="margin-top:20rpx;">我的心愿单</text><view class="banner" hidden="{{!shareflag}}"><block wx:if="{{carts.length>0}}"><form data-name="onShareAppMessage" report-submit="true" data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><button class="btnshare" formType="submit" data-event-opts="{{[['tap',[['shareToWXminiProgram',['$event']]]]]}}" bindtap="__e"><image class="shareto" src="../../static/images/shareto.png"></image>分享</button></form></block><form data-name="addWish" report-submit="true" data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><button class="btnadd" formType="submit"></button></form></view></view><view style="{{('margin-bottom:50rpx;height:100%;')}}" scroll-y="true"><view class="container"><view class="venues_box"><block wx:for="{{carts}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="venues_item"><navigator url="{{'/pages/details/details?id='+item.goods_id+'&image='+(item.activity_image?item.activity_image:item.image)+'&info='+item.act_info+'&name='+item.name+'&gov_price='+item.gov_price+'&goods_price='+item.sell_price+'&sale='+item.sale}}" hover-class="none"><image class="mainpic" src="{{item.activity_image?item.activity_image:item.image}}"></image></navigator><text class="goods_name">{{item.name}}</text><view class="goods_sku"><block wx:for="{{item['value']}}" wx:for-item="sku_value" wx:for-index="index2" wx:key="index2"><view><text>{{sku_value['type']==2?sku_value['note']+' ':sku_value['value']+' '}}</text></view></block></view><view class="price-list"><view class="price-market">{{item.market_price>0?'￥'+item.market_price:''}}</view><view class="price-now">{{"￥"+item.sell_price}}</view></view><view class="goods_bottom"><view>{{"已收到:"+(item.sale?item.sale:0)+"件"}}</view><view hidden="{{!shareflag}}"><image style="width:40rpx;height:40rpx;" data-index="{{index}}" data-object-id="{{item.objectId}}" src="../../static/images/icon_delete.png" data-event-opts="{{[['tap',[['deleteTapTag',['$event']]]]]}}" bindtap="__e"></image></view></view><view style="text-align:right;margin-top:-50rpx;margin-right:10rpx;" hidden="{{shareflag}}"><button class="button" style="margin-top:10px;" data-index="{{index}}" data-object-id="{{item.objectId}}" type="warn" size="mini" hover-class="button-hover" data-event-opts="{{[['tap',[['sendGoodsTapTag',['$event']]]]]}}" bindtap="__e">送Ta</button></view></view></block></block></view></view></view></view>