<view><block wx:if="{{is_buymyself==0&&goodsshape!=5}}"><view class="subject"><text class="toptitle">{{goodsshape==5?goodsname+'已完成':'礼物已为您打好包'}}</text><text>选择微信好友发送给对方</text></view></block><block wx:if="{{is_buymyself==0&&goodsshape!=5&&goodsshape!=4}}"><view class="order-item"><view class="order-item-banner"><view><image style="width:378rpx;height:202rpx;" src="../../../static/images/gift.gif" mode="aspectFill"></image></view><view class="userinfo"><image class="userinfo-avatar" src="{{userInfo.avatarUrl}}" background-size="cover"></image><text class="userinfo-nickname">{{userInfo.nickname}}</text></view><view class="order_note"><text space="ensp">{{note}}</text></view><block wx:for="{{orderskus}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="gifts-item"><view data-sku-id="{{item.sku_id}}" data-goods-id="{{item.goods_id}}" data-goods-name="{{item.goods_name}}" data-event-opts="{{[['tap',[['showGoods',['$event']]]]]}}" bindtap="__e"><image class="gifts-image" src="{{item.sku_image}}" mode="aspectFill"></image></view><text class="carts-title">{{item.goods_name+" x"+item.sku_num}}</text></view></block></view><view class="sentbtn"><button open-type="share" hidden="{{send_status==0?false:true}}" data-event-opts="{{[['tap',[['shareToWXminiProgram',['$event']]]]]}}" bindtap="__e">选择微信好友</button><view hidden="{{send_status==0?true:false}}"><form report-submit="true" data-event-opts="{{[['submit',[['returnForm',['$event']]]]]}}" bindsubmit="__e"><button style="width:300rpx;" formType="submit">完成</button></form></view></view></view></block></view>