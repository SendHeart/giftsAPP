<view><view class="top-bar2"><block wx:for="{{navList_order}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="{{['top-bar-item2 '+(index==TabCur?'top-bar-active2':'')]}}" id="{{'v_'+index}}" data-id="{{index}}" data-title="{{item.title}}" data-tab="{{item.id}}" data-event-opts="{{[['tap',[['onOrderTapTag',['$event']]]]]}}" catchtap="__e"><view>{{item.title}}</view></view></block></block></view><view class="order-item-banner"><block wx:for="{{coupons_list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['couponTapTag',['$event']]]]]}}" class="coupons" style="{{('background-image:url('+$1+');')}}" bindtap="__e"><text class="coupon-name">{{item.name+(giftflag==0?'[未使用]':'')+(giftflag==1?'[已使用]':'')+(giftflag==2?'[已过期]':'')}}</text><text class="coupon-content">{{item.content}}</text><text class="coupon-content">{{item.amount>0?'￥'+item.amount:''}}</text><text class="coupon-footer">{{item.footer}}</text><text class="coupon-dueday">{{(item.start_time?'(有效期:'+item.start_time+'至':'')+(item.end_time?item.end_time+')':'')}}</text></view></block><block wx:if="{{page_num>1}}"><button class="morebtn" type="default" size="default" hover-class="none" data-event-opts="{{[['tap',[['getMoreOrdersTapTag',['$event']]]]]}}" bindtap="__e">{{"更多("+page+"/"+page_num+")"}}</button></block></view></view>