<view><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="cell"><text>区域</text><text data-event-opts="{{[['tap',[['cascadePopup',['$event']]]]]}}" class="area_select" bindtap="__e">{{areaSelectedStr}}</text><text data-event-opts="{{[['tap',[['fetchPOI',['$event']]]]]}}" class="poi" bindtap="__e">自动获取</text></view><view class="cell"><text>地址</text><input name="detail" placeholder="请输入详情地址" value="{{address.address}}"/></view><block wx:if="{{is_activity==1}}"><view class="cell"><text>备注</text><input name="activity_name" placeholder="请输入备注" value="{{address.name}}"/></view></block><view class="cell"><text>收件人</text><input name="full_name" placeholder="请输入收件人姓名" type="text" value="{{address.full_name}}"/></view><view class="cell"><text>手机号</text><input name="mobile" placeholder="请输入手机号码" type="number" value="{{address.tel}}"/></view><block wx:if="{{is_activity==1}}"><view class="cell"><text>侯时</text><input name="waitting" placeholder="请输入等候时长(小时)" type="digit" value="{{address.waitting}}"/></view></block><view class="upimage"><block wx:for="{{img_arr}}" wx:for-item="upimg" wx:for-index="index" wx:key="index"><block><image data-id="{{index}}" src="{{upimg}}" mode="aspectFit" data-event-opts="{{[['tap',[['cancel_upimg',['$event']]]]]}}" bindtap="__e"></image></block></block><block wx:for="{{new_img_arr}}" wx:for-item="upimg_new" wx:for-index="index" wx:key="index"><block><image data-id="{{index}}" src="{{upimg_new}}" mode="aspectFit" data-event-opts="{{[['tap',[['cancel_new_upimg',['$event']]]]]}}" bindtap="__e"></image></block></block></view><block wx:if="{{is_activity==1}}"><view data-event-opts="{{[['tap',[['upimg',['$event']]]]]}}" class="upimg" bindtap="__e"><image class="btnjia" src="../../../static/images/jia.png"></image></view></block><button class="save" formType="submit" type="warn">保存</button></form><view class="modal"><view data-event-opts="{{[['tap',[['cascadeDismiss',['$event']]]]]}}" class="{{['modal-mask '+maskVisual]}}" bindtap="__e"></view><view class="modal-content" animation="{{animationData}}"><view class="modal-header"><text class="modal-title">所在地区</text><text data-event-opts="{{[['tap',[['cascadeDismiss',['$event']]]]]}}" class="modal-close" bindtap="__e">X</text></view><view class="modal-body"><text class="{{['viewpager-title '+(current==0?'area-selected':'')]}}" data-current="0" data-event-opts="{{[['tap',[['changeCurrent',['$event']]]]]}}" bindtap="__e">{{provinceName}}</text><text class="{{['viewpager-title '+(current==1?'area-selected':'')]}}" data-current="1" data-event-opts="{{[['tap',[['changeCurrent',['$event']]]]]}}" bindtap="__e">{{cityName}}</text><text class="{{['viewpager-title '+(current==2?'area-selected':'')]}}" data-current="2" data-event-opts="{{[['tap',[['changeCurrent',['$event']]]]]}}" bindtap="__e">{{regionName}}</text><text class="{{['viewpager-title '+(current==3?'area-selected':'')]}}" data-current="3" data-event-opts="{{[['tap',[['changeCurrent',['$event']]]]]}}" bindtap="__e">{{townName}}</text><view class="viewpager-divider"></view><swiper class="swiper-area" current="{{current}}" data-event-opts="{{[['change',[['currentChanged',['$event']]]]]}}" bindchange="__e"><block wx:if="{{province.length>0}}"><block><swiper-item><scroll-view class="viewpager-listview" scroll-y="true"><block wx:for="{{province}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-index="{{index}}" data-event-opts="{{[['tap',[['provinceTapped',['$event']]]]]}}" bindtap="__e"><block wx:if="{{index==provinceIndex}}"><text class="area-selected">{{item}}</text></block><block wx:else><text>{{item}}</text></block></view></block></scroll-view></swiper-item></block></block><block wx:if="{{city.length>0}}"><block><swiper-item><scroll-view class="viewpager-listview" scroll-y="true"><block wx:for="{{city}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-index="{{index}}" data-event-opts="{{[['tap',[['cityTapped',['$event']]]]]}}" bindtap="__e"><block wx:if="{{index==cityIndex}}"><text class="area-selected">{{item}}</text></block><block wx:else><text>{{item}}</text></block></view></block></scroll-view></swiper-item></block></block><block wx:if="{{region.length>0}}"><block><swiper-item><scroll-view class="viewpager-listview" scroll-y="true"><block wx:for="{{region}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-index="{{index}}" data-event-opts="{{[['tap',[['regionTapped',['$event']]]]]}}" bindtap="__e"><block wx:if="{{index==regionIndex}}"><text class="area-selected">{{item}}</text></block><block wx:else><text>{{item}}</text></block></view></block></scroll-view></swiper-item></block></block><block wx:if="{{town.length>0}}"><block><swiper-item><scroll-view class="viewpager-listview" scroll-y="true"><block wx:for="{{town}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-index="{{index}}" data-event-opts="{{[['tap',[['townTapped',['$event']]]]]}}" bindtap="__e"><block wx:if="{{index==townIndex}}"><text class="area-selected">{{item}}</text></block><block wx:else><text>{{item}}</text></block></view></block></scroll-view></swiper-item></block></block></swiper></view></view></view></view>